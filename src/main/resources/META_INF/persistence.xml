<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_2_0.xsd"
             version="2.0">
    <persistence-unit name="persistenceManager">
        <class>com.bft.spring.generatedmodel.UserTableEntity</class>
        <properties>
            <property name="eclipselink.jdbc.url" value="jdbc:postgresql://localhost:5432/test1"/>
            <property name="eclipselink.jdbc.driver" value="org.postgresql.Driver"/>
            <property name="eclipselink.jdbc.user"/>
            <property name="eclipselink.jdbc.password"/>
            <property name="hibernate.connection.url" value="jdbc:postgresql://localhost:5432/test1"/>
            <property name="hibernate.connection.driver_class" value="org.postgresql.Driver"/>
            <property name="hibernate.connection.username"/>
            <property name="hibernate.connection.password"/>
            <property name="openjpa.ConnectionURL" value="jdbc:postgresql://localhost:5432/test1"/>
            <property name="openjpa.ConnectionDriverName" value="org.postgresql.Driver"/>
            <property name="openjpa.ConnectionUserName"/>
            <property name="openjpa.ConnectionPassword"/>
            <property name="toplink.jdbc.url" value="jdbc:postgresql://localhost:5432/test1"/>
            <property name="toplink.jdbc.driver" value="org.postgresql.Driver"/>
            <property name="toplink.jdbc.user"/>
            <property name="toplink.jdbc.password"/>
        </properties>
    </persistence-unit>
    <persistence-unit name="persistenceUnit">
        <class>com.bft.spring.generatedmodel.CompanyEntity</class>
        <properties>
            <property name="eclipselink.jdbc.url" value="jdbc:postgresql://localhost:5432/test1"/>
            <property name="eclipselink.jdbc.driver" value="org.postgresql.Driver"/>
            <property name="eclipselink.jdbc.user"/>
            <property name="eclipselink.jdbc.password"/>
            <property name="hibernate.connection.url" value="jdbc:postgresql://localhost:5432/test1"/>
            <property name="hibernate.connection.driver_class" value="org.postgresql.Driver"/>
            <property name="hibernate.connection.username"/>
            <property name="hibernate.connection.password"/>
            <property name="openjpa.ConnectionURL" value="jdbc:postgresql://localhost:5432/test1"/>
            <property name="openjpa.ConnectionDriverName" value="org.postgresql.Driver"/>
            <property name="openjpa.ConnectionUserName"/>
            <property name="openjpa.ConnectionPassword"/>
            <property name="toplink.jdbc.url" value="jdbc:postgresql://localhost:5432/test1"/>
            <property name="toplink.jdbc.driver" value="org.postgresql.Driver"/>
            <property name="toplink.jdbc.user"/>
            <property name="toplink.jdbc.password"/>
        </properties>
